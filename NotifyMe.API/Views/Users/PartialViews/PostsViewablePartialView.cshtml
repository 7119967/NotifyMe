<table class="table">
    <thead class="">
    <tr>
        <th></th>
        <th></th>
        <th></th>
        <th></th>
    </tr>
    </thead>
    <tbody>
    @foreach (var item in Model.Posts)
    {
        <tr>
            <th scope="row">@item.Id</th>
            <td><img src="@item.Image" style="width: 100px; height: 100px;" alt=""></td>
            <td>@item.TimeStamp.ToShortDateString()</td>
            <td>@item.Description</td>
            <td>
                <button id="deleting" class="btn btn-outline-danger" onclick="deleting('@item.Id', '@item.User.Id')">Удалить</button>
                <button id="editing" class="btn btn-outline-secondary" onclick="editing('@item.Id')">Редактировать</button>
            </td>
        </tr>
    }
    </tbody>
</table>