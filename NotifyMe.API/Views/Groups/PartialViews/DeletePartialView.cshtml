@model GroupDeleteViewModel

@{
    ViewBag.Title = "Delete";
    Layout = "_Layout";
}
<div class="modal fade" id="modal-delete" tabindex="-1" aria-labelledby="modalDelete" aria-hidden="true">
    <div class="modal-dialog modal-lg p-5">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="modalDelete">@ViewBag.Title</h5>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
            </div>
            <div class="modal-body">
                <p>Are you sure you want to delete this @Model.Id item?</p>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-danger" data-bs-dismiss="modal">Cancel</button>
                <button type="submit" class="btn btn-success" onclick="deleting(@Model.Id)">Delete</button>
            </div>
        </div>
    </div>
</div>